---
pagination:
  data: Compounds
  size: 1
permalink: 'cid/{{ pagination.items[0].Cid}}/index.html'
layout: "base.njk"
---

<div >
  <h2>{{ pagination.items[0].Title }}</h2>
  <div class="compound-description">{{ pagination.items[0].Description }}</div>
  <div ><img class="structure-image" alt="{{pagination.items[0].Title}}" src="/images/CID_{{pagination.items[0].Cid}}.png" ></div>
  <div  >
    <div>Molecular Formula: {{ pagination.items[0].Molecular_Formula }}</div>
    <div>Molecular wt: {{ pagination.items[0].Molecular_Weight }} g/mole</div>
    <div class="text-with-icon"><img class="icon" title="SMILES:" alt="SMILES" src="/images/Smile.png">: {{pagination.items[0].SMILES_Canonical}}</div>
    <div>{{pagination.items[0].InChI_Standard}}</div>
    <div>InChIKey: {{pagination.items[0].InChIKey_Standard}}</div>
  </div>
  <div class="row-of-links">
    <div  ><a href="{{pagination.items[0].DescriptionURL}}" target="_blank">{{pagination.items[0].DescriptionSourceName}}</a> </div>
    <div  ><a href="https://pubchem.ncbi.nlm.nih.gov/compound/{{pagination.items[0].Cid}}" target="_blank">PubChem</a></div>
  </div>
</div>
