---
pagination:
  data: Compounds
  size: 1
  alias: compoundPageData
  addAllPagesToCollections: true
permalink: 'cid/{{ compoundPageData.Cid}}/index.html'
layout: "base.njk"
eleventyComputed:
  eleventyNavigation:
    key: "{{ compoundPageData.Title }}"
---

<div >
  <h2>{{ compoundPageData.Title }}</h2>
  <div class="compound-description">{{ compoundPageData.Description }}</div>
  <div ><img class="structure-image" alt="{{compoundPageData.Title}}" src="/images/CID_{{compoundPageData.Cid}}.png" ></div>
  <div  >
    <div>Molecular Formula: {{ compoundPageData.Molecular_Formula }}</div>
    <div>Molecular wt: {{ compoundPageData.Molecular_Weight }} g/mole</div>
    <div>{{compoundPageData.SMILES_Canonical}}</div>
    <div>{{compoundPageData.InChI_Standard}}</div>
    <div>InChIKey: {{compoundPageData.InChIKey_Standard}}</div>
  </div>
  <div class="row-of-links">
    <div  ><a href="{{compoundPageData.DescriptionURL}}" target="_blank">{{compoundPageData.DescriptionSourceName}}</a> </div>
    <div  ><a href="https://pubchem.ncbi.nlm.nih.gov/compound/{{compoundPageData.Cid}}" target="_blank">PubChem</a></div>
  </div>
</div>
