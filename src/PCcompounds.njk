---
pagination:
  data: Compounds
  size: 1
  alias: compound
  addAllPagesToCollections: true
permalink: 'cid/{{ compound.Cid}}/index.html'
layout: "base.njk"
eleventyComputed:
  eleventyNavigation:
    key: "{{ compound.Title }}"
---

<div >
  <h2>{{ compound.Title }}</h2>
  <div class="compound-description">{{ compound.Description }}</div>
  <div ><img class="structure-image" alt="{{compound.Title}}" src="/images/CID_{{compound.Cid}}.png" ></div>
  <div  >
    <div>Molecular Formula: {{ compound.Molecular_Formula }}</div>
    <div>Molecular wt: {{ compound.Molecular_Weight }} g/mole</div>
    <div>{{compound.SMILES_Canonical}}</div>
    <div>{{compound.InChI_Standard}}</div>
    <div>InChIKey: {{compound.InChIKey_Standard}}</div>
  </div>
  <div class="row-of-links">
    <div  ><a href="{{compound.DescriptionURL}}" target="_blank">{{compound.DescriptionSourceName}}</a> </div>
    <div  ><a href="https://pubchem.ncbi.nlm.nih.gov/compound/{{compound.Cid}}" target="_blank">PubChem</a></div>
  </div>
</div>
